# WebSocket Backend URL for real-time code generation
# Default: ws://127.0.0.1:7001
# Note: WebSocket must be on the SAME PORT as HTTP backend
VITE_WS_BACKEND_URL=ws://127.0.0.1:7001

# HTTP Backend URL for REST API calls
# Default: http://127.0.0.1:7001
VITE_HTTP_BACKEND_URL=http://127.0.0.1:7001

# Deployment mode (set to "true" when running on cloud/production)
# Default: false (local development)
VITE_IS_DEPLOYED=false
