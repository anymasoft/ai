# JobRadar v0 - Пример конфигурации
# Скопируй этот файл в .env и заполни своими значениями

# Telegram Bot Token (получить у @BotFather)
TELEGRAM_BOT_TOKEN=1234567890:ABCDefGHIJklmnopQRSTuvwxyz

# Telegram API ID (получить на https://my.telegram.org/apps)
TELEGRAM_API_ID=123456

# Telegram API Hash (получить на https://my.telegram.org/apps)
TELEGRAM_API_HASH=abcdefghijklmnopqrstuvwxyzabcdef

# Номер телефона для User Client (включая код страны)
# Пример: +79991234567
TELEGRAM_PHONE=+79991234567

# Telegram ID админа (твой личный ID)
# Узнать свой ID можно через @userinfobot
TELEGRAM_ADMIN_ID=123456789

# Режим отладки (True/False)
DEBUG=False
